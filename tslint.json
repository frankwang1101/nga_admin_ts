{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": [
      false,
      "dir",
      "log",
      "error",
      "warn"
    ],
    "ordered-imports": false,
    "interface-over-type-literal": false
  }
}
